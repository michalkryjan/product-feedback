{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "./",
    "verbatimModuleSyntax": false,
    "paths": {
      "#imports": [
        "./.nuxt/imports.d.ts"
      ],
      "types/*": [
        "./types/*"
      ],
      "*": [
        "types/*.d.ts"
      ],
      "~/*": [
        "./*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ]
}
