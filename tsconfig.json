{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "verbatimModuleSyntax": false,
    "types": [
      "@pinia/nuxt"
    ],
    "paths": {
      "types/*": [
        "./types/*"
      ],
      "components/*": [
        "./components/*"
      ],
      "*": [
        "types/*.d.ts"
      ],
      "#imports": [
        ".nuxt/imports.d.ts"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}
