{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "verbatimModuleSyntax": false,
    "types": [
      "@pinia/nuxt"
    ],
    "typeRoots": [
      "./node_modules",
      "./types"
    ],
    "paths": {
      "*": ["types/*.d.ts"],
      "#imports": [
        ".nuxt/imports.d.ts"
      ]
    },
  }
}
